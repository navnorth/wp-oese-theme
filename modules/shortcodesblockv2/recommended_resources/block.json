{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "oese-block/oese-recommended-resources-block",
	"version": "0.1.0",
	"title": "Recommended Resources",
	"category": "oese-block-category",
	"icon": "playlist-video",
	"description": "Add Recommended Resources Block",
	"supports": {
		"html": false
	},
	"attributes":{
		"blockid": {"type":"string"},
		"blockclientid": {"type":"string"},
    "youtubeid":{"type": "string"},
		"description":{"type": "string"},
		"descriptionRows":{"type":"integer","default": 1},
		"oeseblkRecommendedResourcesHeading":{"type": "string"},
		"oeseblkRecommendedResourcesIsVideo": {"type": "boolean","default": true},
		"oeseblkRecommendedResourcesWebsiteUrl": {"type": "string", "default": ""},
		"oeseblkRecommendedResourcesWebsiteImageID": {"type":"number"},
		"oeseblkRecommendedResourcesWebsiteImageUrl": {"type": "string"},
		"oeseblkRecommendedResourcesOpeninnewtab": {"type": "boolean","default": true}
	},
	"textdomain": "oese-recommended-resources-block",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}